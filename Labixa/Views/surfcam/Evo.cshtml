
@{
    ViewBag.Title = "Evo";
    ViewBag.Description = "SURFCAM Evolution Phay có thể tạo đường dẫn dao cho gia công 3 đến 5 trục, có thể được sử dụng với các bề mặt cong phức tạp từ 2,5 lõi cho khung dây hoặc mô hình rắn.";
    ViewBag.Image = "https://datadesign.vn/Content/surfcam/images/logo/Surfcam_logo_small1.png";
    ViewBag.Url = "https://datadesign.vn/surfcam/evo";
    Layout = "~/Views/Shared/_Surfcam.cshtml";
    HttpCookie cookie = Request.Cookies["_culture"];
}
@model Outsourcing.Data.Models.Product

@section scripts{


    <link rel="stylesheet" href="/Content/surfcam/css/body_styles.css">
    <link rel="stylesheet" href="/Content/surfcam/cssmenu/styles.css">

    <!-- Đánh dấu JSON-LD được tạo bởi Trình trợ giúp đánh dấu dữ liệu có cấu trúc của Google. -->
    <script type="application/ld+json">
        {
        "@@context" : "http://schema.org",
        "@@type" : "Product",
        "name" : "evo",
        "image" : "https://datadesign.vn/Content/surfcam/images/Surfcam/795316b92fc766b0181f6fef074f03fa.png",
        "description" : "SURFCAM Evolution Milling can create a tool path for 3-to-5-axis machining, which can be used with complex curved surfaces ranging from 2.5 cores for wire frame or solid model.",
        "url" : "https://datadesign.vn/surfcam/evo",
        "brand" : {
        "@@type" : "Brand",
        "name" : "surfcam",
        "logo" : "https://datadesign.vn/Content/surfcam/images/logo/Surfcam_logo_small.png"
        }
        }
    </script>
}
@if (Model != null)
{
    if (cookie.Value == ("vi"))
    {
        @Html.Raw(Model.Content)
    }
    else
    {
        @Html.Raw(Model.ContentEng)
    }
}



