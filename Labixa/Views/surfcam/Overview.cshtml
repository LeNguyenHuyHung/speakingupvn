
@{
    ViewBag.Title = "Overview-Surfcam-DataDesignVN";
    ViewBag.Description = "SURFCAM là một nguyên mẫu của xử lý khuôn toàn diện. Hệ thống CAD / CAM 3D đóng vai trò hiệu quả trong nhiều lĩnh vực như xử lý các bộ phận và bộ phận kết cấu. Trong phần cắt, để tạo ra dữ liệu tuyến để đáp ứng nhu cầu xử lý lên đến tinh tế, tốc độ cao, độ chính xác cao, hiệu quả cao khi gia công đồng thời 5 trục từ 2 lõi tương ứng với nhiều ứng dụng xử lý khác nhau để xử lý gia công phức tạp. Nhà phát triển: SURFCAM.Inc";
    ViewBag.Image = "https://datadesign.vn/Content/surfcam/images/Surfcam/Surfcam_Waveform.png";
    ViewBag.Url = "https://datadesign.vn/surfcam/overview";
    Layout = "~/Views/Shared/_Surfcam.cshtml";
    HttpCookie cookie = Request.Cookies["_culture"];
}
@model Outsourcing.Data.Models.Product

@section scripts{

    <link rel="stylesheet" href="/Content/surfcam/css/body_styles.css">
    <link rel="stylesheet" href="/Content/surfcam/cssmenu/styles.css">

    <!-- Đánh dấu JSON-LD được tạo bởi Trình trợ giúp đánh dấu dữ liệu có cấu trúc của Google. -->
    <script type="application/ld+json">
        {
        "@@context" : "http://schema.org",
        "@@type" : "Product",
        "name" : "overview",
        "image" : "https://datadesign.vn/Content/surfcam/images/Surfcam/sc-slider.jpg",
        "description" : "WorkNC is a CAM software that will enhance the design and manufacturing capabilities in various industries with a focus on automation. Low error and easy to use. With this principle, it allows employees who have experience working with CNC machines to learn and create toolpaths for use at the time they want.",
        "url" : "https://datadesign.vn/surfcam/Overview",
        "brand" : {
        "@@type" : "Brand",
        "name" : "surfcam",
        "logo" : "https://datadesign.vn/Content/surfcam/images/logo/Surfcam_logo_small.png"
        }
        }
    </script>
}
@if (Model != null)
{
    if (cookie.Value == ("vi"))
    {
        @Html.Raw(Model.Content)
    }
    else
    {
        @Html.Raw(Model.ContentEng)
    }
}





