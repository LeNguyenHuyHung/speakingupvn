
@{
    ViewBag.Title = "regdownload";
    Layout = "~/Views/Shared/_Surfcam.cshtml";
    HttpCookie cookie = Request.Cookies["_culture"];
}
@model Outsourcing.Data.Models.Product
@section scripts{

    <link rel="stylesheet" href="/Content/surfcam/css/body_styles.css">
    <link rel="stylesheet" href="/Content/surfcam/cssmenu/styles.css">
    <link rel="stylesheet" href="/Content/Artec3D/css/reponsive_mobile.css">

    <!-- Đánh dấu JSON-LD được tạo bởi Trình trợ giúp đánh dấu dữ liệu có cấu trúc của Google. -->
    <script type="application/ld+json">
        {
        "@@context" : "http://schema.org",
        "@@type" : "Product",
        "name" : "regdownload",
        "description" : "Download Program Try Version",
        "url" : "https://datadesign.vn/surfcam/regdownload",
        "brand" : {
        "@@type" : "Brand",
        "name" : "surfcam",
        "logo" : "https://datadesign.vn/Content/surfcam/images/logo/Surfcam_logo_small.png"
        }
        }
    </script>
}
@if (Model != null)
{
    if (cookie.Value == ("vi"))
    {
        @Html.Raw(Model.Content)
    }
    else
    {
        @Html.Raw(Model.ContentEng)
    }
}

