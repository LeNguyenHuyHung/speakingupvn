
@{
    ViewBag.Title = "Surfcam-DataDesignVN";
    ViewBag.Description = "SURFCAM là mô-đun CAM tự động của các thành phần rắn. Các mô-đun CAM dựa trên bề mặt Tập trung vào việc sử dụng và học các phương pháp truyền thống Và nhận ra các đề xuất tốt nhất theo mục đích và ứng dụng Là một phần mềm CAM dễ sử dụng.";
    ViewBag.Image = "https://datadesign.vn/Content/surfcam/images/logo/Surfcam_logo_small1.png";
    ViewBag.Url = "https://datadesign.vn/surfcam/index";
    Layout = "~/Views/Shared/_Surfcam.cshtml";
    HttpCookie cookie = Request.Cookies["_culture"];
}
@model Outsourcing.Data.Models.Blog

@section scripts{

    <link rel="stylesheet" href="/Content/surfcam/css/vdo_background.css">
    <link rel="stylesheet" href="/Content/surfcam/cssmenu/styles.css">
    <link rel="stylesheet" href="/Content/Artec3D/css/reponsive_mobile.css">

    <!-- Đánh dấu JSON-LD được tạo bởi Trình trợ giúp đánh dấu dữ liệu có cấu trúc của Google. -->
    <script type="application/ld+json">
        {
        "@@context" : "http://schema.org",
        "@@type" : "Product",
        "name" : "surfcam",
        "description" : "Consisting of advanced 3D-CAM technology and outstanding cutting technology,the new CAM system has changed the concept of processing parts considerably.",
        "url" : "https://datadesign.vn/surfcam",
        "brand" : {
        "@@type" : "Brand",
        "name" : "surfcam",
        "logo" : "https://datadesign.vn/Content/surfcam/images/logo/Surfcam_logo_small.png"
        }
        }
    </script>
}

<video autoplay muted loop id="myVideo">
    <source src="/Content/surfcam/vdo/2018-11-27_15-27-37.mp4" type="video/mp4">
    Your browser does not support HTML5 video.
</video>
@if (Model != null)
{
    if (cookie.Value == ("vi"))
    {
        @Html.Raw(Model.Content)
    }
    else
    {
        @Html.Raw(Model.ContentEng)
    }
}

