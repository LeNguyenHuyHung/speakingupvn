
@{
    ViewBag.Title = "GeoMagic Wrap";
    ViewBag.Description = "Geomagic Wrap 2017 mang lại quy trình quét cho mô hình 3D được cập nhật. Từ hỗ trợ định dạng tệp mở rộng đến thao tác trên bản đồ bề mặt để làm cho các phần được in trở nên sống động, hộp công cụ 3D Geomagic Wrap cung cấp nhiều khả năng.";
    ViewBag.Image = "https://datadesign.vn/Content/Artec3D/images/geomagic/geomagic-wrap/geomagic-wrap-title-img1.png";
    ViewBag.Url = "https://datadesign.vn/geoMagicWrap";
    Layout = "~/Views/Shared/_Artec3D.cshtml";
    HttpCookie cookie = Request.Cookies["_culture"];
}
@model Outsourcing.Data.Models.Product
@section scripts{


    <link rel="stylesheet" href="/Content/Artec3D/css/body-styles1.css">
    <link rel="stylesheet" href="/Content/Artec3D/css/geomagic-wrap-styles.css">
    <link rel="stylesheet" href="/Content/Artec3D/css/reponsive_mobile.css">
    <link rel="stylesheet" href="/Content/Artec3D/cssmenu/styles.css">

    <!-- Đánh dấu JSON-LD được tạo bởi Trình trợ giúp đánh dấu dữ liệu có cấu trúc của Google. -->
    <script type="application/ld+json">
        {
        "@@context" : "http://schema.org",
        "@@type" : "Product",
        "name" : "Geomagic Wrap 2017",
        "image" : "https://datadesign.vn/Content/Artec3D/images/geomagic/geomagic-wrap/geomagic-wrap-title-img.png",
        "description" : "A truly useful surface solution with a comprehensive tool box.",
        "url" : "https://datadesign.vn/geoMagicWrap",
        "brand" : {
        "@@type" : "Brand",
        "name" : "Artec3D",
        "logo" : "https://datadesign.vn/Content/Artec3D/images/artec/3dPrintingartec3d-logo.png"
        }
        }
    </script>
}
@if (Model != null)
{
    if (cookie.Value == "vi")
    {
        @Html.Raw(Model.Content)
    }
    else
    {
        @Html.Raw(Model.ContentEng)
    }
}
