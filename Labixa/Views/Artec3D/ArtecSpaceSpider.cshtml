
@{
    ViewBag.Title = "Artec Space Spider";
    ViewBag.Description = "Một công cụ giúp cải tiến và có độ chính xác cao cho người dùng CAD cùng với các kỹ thuật viên. Artec Space Spider quét hình ảnh 3D với độ phân giải cao sử dụng công nghệ ánh sáng xanh phù hợp cho việc thu hình của các vật thể nhỏ hoặc có các chi tiết phức tạp ...";
    ViewBag.Image = "https://datadesign.vn/Content/Artec3D/images/artec/space_spider/artec-spider-big-en1.png";
    ViewBag.Url = "https://datadesign.vn/artecSpaceSpider";
    Layout = "~/Views/Shared/_Artec3D.cshtml";
    HttpCookie cookie = Request.Cookies["_culture"];
}
@model Outsourcing.Data.Models.Product
@section scripts{
    <link rel="stylesheet" href="/Content/Artec3D/css/body-styles1.css">
    <link rel="stylesheet" href="/Content/Artec3D/css/space-spider-styles.css">
    <link rel="stylesheet" href="/Content/Artec3D/css/reponsive_mobile.css">
    <link rel="stylesheet" href="/Content/worknc/css/reponsive_pc.css">
    <link rel="stylesheet" href="/Content/Artec3D/cssmenu/styles.css">
    <!-- Đánh dấu JSON-LD được tạo bởi Trình trợ giúp đánh dấu dữ liệu có cấu trúc của Google. -->
    <script type="application/ld+json">
        {
        "@@context" : "http://schema.org",
        "@@type" : "Product",
        "name" : "Artec Space Spider",
        "image" : "https://datadesign.vn/Content/Artec3D/images/artec/space_spider/artec-spider-big-en.png",
        "description" : "3D scanner for industrial applications with high accuracy",
        "url" : "https://datadesign.vn/artecSpaceSpider",
        "brand" : {
        "@@type" : "Brand",
        "name" : "Artec3D",
        "logo" : "https://datadesign.vn/Content/Artec3D/images/artec/3dPrintingartec3d-logo.png"
        }
        }
    </script>
}
@if (Model != null)
{
    if (cookie.Value == "vi")
    {
        @Html.Raw(Model.Content)
    }
    else
    {
        @Html.Raw(Model.ContentEng)
    }
}

