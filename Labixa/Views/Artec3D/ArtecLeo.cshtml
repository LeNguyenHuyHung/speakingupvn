
@{
    ViewBag.Title = "Artec Leo";
    ViewBag.Description = "Artec Leo là máy quét 3D đầu tiên xử lý dữ liệu tự động mang đến quy trình trực quan nhất, biến việc quét 3D trở nên dễ dàng như quay video. Khi quét, người dùng sẽ thấy thời gian thực hiện trên màn hình cảm ứng của Leo...";
    ViewBag.Image = "https://datadesign.vn/Content/Artec3D/images/artec/artec-leo/artec-leo-en1.png";
    ViewBag.Url = "https://datadesign.vn/artecLeo";
    Layout = "~/Views/Shared/_Artec3D.cshtml";
    HttpCookie cookie = Request.Cookies["_culture"];
}
@model Outsourcing.Data.Models.Product
@section scripts{
    <link rel="stylesheet" href="/Content/Artec3D/css/body-styles1.css">
    <link rel="stylesheet" href="/Content/Artec3D/css/artec-leo.css">
    <link rel="stylesheet" href="/Content/Artec3D/css/reponsive_mobile.css">
    <link rel="stylesheet" href="/Content/Artec3D/cssmenu/styles.css">
    <link rel="stylesheet" href="/Content/worknc/css/reponsive_pc.css">
    <!-- Đánh dấu JSON-LD được tạo bởi Trình trợ giúp đánh dấu dữ liệu có cấu trúc của Google. -->
    <script type="application/ld+json">
        {
        "@@context" : "http://schema.org",
        "@@type" : "Product",
        "name" : "Artec Leo",
        "image" : "https://datadesign.vn/Content/Artec3D/images/artec/artec-leo/artec-leo-en.png",
        "description" : "Simple 3D scanning: See your 3D objects projected directly on the HD display.",
        "url" : "https://datadesign.vn/artecLeo",
        "brand" : {
        "@@type" : "Brand",
        "name" : "Artec3D",
        "logo" : "https://datadesign.vn/Content/Artec3D/images/artec/3dPrintingartec3d-logo.png"
        }
        }
    </script>
}
@if (Model != null)
{
    if (cookie.Value == "vi")
    {
        @Html.Raw(Model.Content)
    }
    else
    {
        @Html.Raw(Model.ContentEng)
    }
}
