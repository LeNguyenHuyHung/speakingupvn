
@{
    ViewBag.Title = "Artec SDK";
    ViewBag.Description = "Bộ công cụ và thư viện Artec Scanning SDK 2.0 mạnh mẽ cho phép bạn phát triển ứng dụng quét của riêng mình để kiểm soát máy quét 3D Artec của bạn và xử lý dữ liệu đã chụp. Thêm hỗ trợ máy quét Artec vào phần mềm của bạn hoặc Tạo plugin C ++ cho bất kỳ ngành nào.";
    ViewBag.Image = "https://datadesign.vn/Content/Artec3D/images/artec/sdk/sdk-key-post-processing1.jpg";
    ViewBag.Url = "https://datadesign.vn/artecSDK";
    Layout = "~/Views/Shared/_Artec3D.cshtml";
    HttpCookie cookie = Request.Cookies["_culture"];
}
@model Outsourcing.Data.Models.Product
@section scripts{

    <link rel="stylesheet" href="/Content/Artec3D/css/body-styles1.css">
    <link rel="stylesheet" href="/Content/Artec3D/css/sdk_styles.css">

    <link rel="stylesheet" href="/Content/Artec3D/cssmenu/styles.css">
    <!-- Đánh dấu JSON-LD được tạo bởi Trình trợ giúp đánh dấu dữ liệu có cấu trúc của Google. -->
    <script type="application/ld+json">
        {
        "@@context" : "http://schema.org",
        "@@type" : "Product",
        "name" : "Artec Scanning SDK 2.0",
        "image" : "https://datadesign.vn/Content/Artec3D/images/artec/sdk/artec3d-core-productpage-sdk.jpg",
        "description" : "The powerful Artec Scanning SDK 2.0 toolkit and library allows you to develop your own scanning app to control your Artec 3D scanner and process captured data.",
        "url" : "https://datadesign.vn/artecSDK",
        "brand" : {
        "@@type" : "Brand",
        "name" : "Artec3D",
        "logo" : "https://datadesign.vn/Content/Artec3D/images/artec/3dPrintingartec3d-logo.png"
        }
        }
    </script>
}

@if (Model != null)
{
    if (cookie.Value == "vi")
    {
        @Html.Raw(Model.Content)
    }
    else
    {
        @Html.Raw(Model.ContentEng)
    }
}