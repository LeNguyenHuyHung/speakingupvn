
@{
    ViewBag.Title = "Overview3d Artec3D";
    ViewBag.Description = "Artec 3D Scanner là một máy ảnh 3D cầm tay có thể ghi lại từng video, là hình ảnh ba chiều. Chỉ cần đi bộ xung quanh, đối tượng được camera chụp từ nhiều góc độ khác nhau và đi kèm với một phần mềm tự động kết hợp tất cả các hình ảnh được quét thành một lưới duy nhất.";
    ViewBag.Image = "https://datadesign.vn/Content/Artec3D/images/artec/scanning-step-11.png";
    ViewBag.Url = "https://datadesign.vn/overview3d";
    Layout = "~/Views/Shared/_Artec3D.cshtml";
    HttpCookie cookie = Request.Cookies["_culture"];
}
@model Outsourcing.Data.Models.Product
@section scripts{
    <link rel="stylesheet" href="/Content/Artec3D/css/body-styles1.css">
    <link rel="stylesheet" href="/Content/Artec3D/cssmenu/styles.css">
    <!-- Đánh dấu JSON-LD được tạo bởi Trình trợ giúp đánh dấu dữ liệu có cấu trúc của Google. -->
    <script type="application/ld+json">
        {
        "@@context" : "http://schema.org",
        "@@type" : "Product",
        "name" : "Concept",
        "image" : "https://datadesign.vn/Content/Artec3D/images/artec/scanning-step-1.png",
        "description" : "Artec 3D Scanner is a portable 3D camera that captures each video, which is a three-dimensional image",
        "url" : "https://datadesign.vn/overview3d",
        "brand" : {
        "@@type" : "Brand",
        "name" : "Artec3D",
        "logo" : "https://datadesign.vn/Content/Artec3D/images/artec/3dPrintingartec3d-logo.png"
        }
        }
    </script>
}
@if (Model != null)
{
    if (cookie.Value == "vi")
    {
        @Html.Raw(Model.Content)
    }
    else
    {
        @Html.Raw(Model.ContentEng)
    }
}


