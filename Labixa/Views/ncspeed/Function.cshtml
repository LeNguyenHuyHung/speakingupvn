
@{ 
    ViewBag.Title = "Function-Ncspeed-DataDesignVN";
    ViewBag.Description = "Xem xét các điều kiện xử lý khác nhau Ngoài việc cắt, tăng tốc / giảm tốc Làm chậm các bộ phận mà dụng cụ có xu hướng bị mòn, kéo dài tuổi thọ công cụ Giảm thiệt hại";
    ViewBag.Image = "https://datadesign.vn/Content/ncspeed/images/ncspeed/image0051.png";
    ViewBag.Url = "https://datadesign.vn/ncspeed/function";
    Layout = "~/Views/Shared/_Ncspeed.cshtml";
    HttpCookie cookie = Request.Cookies["_culture"];
}
@model Outsourcing.Data.Models.Product

@section scripts{

    <link rel="stylesheet" href="/Content/ncspeed/css/body_styles.css">
    <link rel="stylesheet" href="/Content/ncspeed/cssmenu/styles.css">

    <!-- Đánh dấu JSON-LD được tạo bởi Trình trợ giúp đánh dấu dữ liệu có cấu trúc của Google. -->
    <script type="application/ld+json">
        {
        "@@context" : "http://schema.org",
        "@@type" : "Product",
        "name" : "function",
        "image" : "https://datadesign.vn/Content/ncspeed/images/ncspeed/image005.png",
        "description" : "Considering various processing conditions In addition to cutting, acceleration / deceleration Slow down the parts that the tool tends to wear, prolong the tool life Reduce damage",
        "url" : "https://datadesign.vn/ncspeed/function",
        "brand" : {
        "@@type" : "Brand",
        "name" : "ncspeed",
        "logo" : "https://datadesign.vn/images/logo/NCspeed_logo.jpg"
        }
        }
    </script>
}
@if (Model != null)
{
    if (cookie.Value == ("vi"))
    {
        @Html.Raw(Model.Content)
    }
    else
    {
        @Html.Raw(Model.ContentEng)
    }
}



