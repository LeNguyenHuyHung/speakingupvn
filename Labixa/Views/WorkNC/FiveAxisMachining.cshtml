
@{
    ViewBag.Title = "FiveAxisMachining";
    ViewBag.Title = "WorkNC-Datadesign Việt Nam";
    ViewBag.Description = "Chỉ cần chỉ định mô hình chuyển đổi cho chiều dài của công cụ để mở rộng, sử dụng công cụ 3 trục để tự động đồng thời đường dẫn công cụ 5 trục, được chấp nhận bởi Chủ sở hữu và chiều dài Công cụ.";
    ViewBag.Url = "https://datadesign.vn/worknc/five-axis-machining";
    Layout = "~/Views/Shared/_WorkNC.cshtml";
    HttpCookie cookie = Request.Cookies["_culture"];
}
@model Outsourcing.Data.Models.Product

@section scripts{

    <link rel="stylesheet" href="/Content/worknc/css/body_styles.css">
    <link rel="stylesheet" href="/Content/worknc/cssmenu/styles.css">
    <link rel="stylesheet" href="/Content/Artec3D/css/reponsive_mobile.css">
    <!-- Đánh dấu JSON-LD được tạo bởi Trình trợ giúp đánh dấu dữ liệu có cấu trúc của Google. -->
    <script type="application/ld+json">
        {
        "@@context" : "http://schema.org",
        "@@type" : "Product",
        "name" : "FiveAxisMachining",
        "image" : "https://datadesign.vn/Content/worknc/images/worknc/4-cam-01.jpg",
        "description" : "You can create tool paths by easily identifying angles. 5-axis machining using accepted indexing for machining deep and undercut parts",
        "url" : "https://datadesign.vn/worknc/function-5-axis",
        "brand" : {
        "@@type" : "Brand",
        "name" : "Worknc",
        "logo" : "https://datadesign.vn/Content/worknc/images/logo/worknc_logo_small.png"
        }
        }
    </script>
}
@if (Model != null)
{
    if (cookie.Value == ("vi"))
    {
        @Html.Raw(Model.Content)
    }
    else
    {
        @Html.Raw(Model.ContentEng)
    }
}


