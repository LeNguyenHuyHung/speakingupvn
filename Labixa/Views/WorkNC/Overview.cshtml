
@{
    ViewBag.Title = "Overview-WorkNC-DataDesignVN";
    ViewBag.Description = "Một quy trình kỹ thuật số mới bắt đầu bằng quét 3D có thể hiển thị một hình ảnh ảo trước mặt bạn.WorkNC là phần mềm hỗ trợ việc chỉnh sửa có thể tự động theo dõi các quá trình tự động hóa phức tạp của cam và có thể dễ dàng tạo ra dữ liệu xử lý chất lượng với chỉ một nút.";
     ViewBag.Image = "https://datadesign.vn/Content/worknc/images/logo/worknc_logo_small1.png";
    ViewBag.Url = "https://datadesign.vn/WorkNC/Overview";
    Layout = "~/Views/Shared/_WorkNC.cshtml";
    HttpCookie cookie = Request.Cookies["_culture"];
}
@model Outsourcing.Data.Models.Product

@section scripts{

    <link rel="stylesheet" href="/Content/worknc/css/body_styles.css">
    <link rel="stylesheet" href="/Content/worknc/cssmenu/styles.css">
    <link rel="stylesheet" href="/Content/Artec3D/css/reponsive_mobile.css">

    <!-- Đánh dấu JSON-LD được tạo bởi Trình trợ giúp đánh dấu dữ liệu có cấu trúc của Google. -->
    <script type="application/ld+json">
        {
        "@@context" : "http://schema.org",
        "@@type" : "Product",
        "name" : "Overview",
        "image" : "https://datadesign.vn/Content/worknc/images/worknc/wnc.png",
        "description" : "WorkNC is a CAM software that will enhance the design and manufacturing capabilities in various industries with a focus on automation. Low error and easy to use. With this principle, it allows employees who have experience working with CNC machines to learn and create toolpaths for use at the time they want.",
        "url" : "https://datadesign.vn/WorkNC/Overview",
        "brand" : {
        "@@type" : "Brand",
        "name" : "Worknc",
        "logo" : "https://datadesign.vn/Content/worknc/images/logo/worknc_logo_small.png"
        }
        }
    </script>
}

@if (Model != null)
{
    if (cookie.Value == ("vi"))
    {
        @Html.Raw(Model.Content)
    }
    else
    {
        @Html.Raw(Model.ContentEng)
    }
}
