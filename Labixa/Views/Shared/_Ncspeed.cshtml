<body>
    @Html.Action("HomeCommon", "Home")
    <div class="top_bar">
        <div class="logo">
            <div class="table-logo">
                <a href="/home"><img src="~/Content/Datadesign-home/Data-Design-Viet-Nam.png" id="logo-size" alt="Data Design Viet Nam" /></a>

                <div class="title-top-right"><img src="../images/logo/NCspeed_logo.jpg" id="logo-nc-size" alt="Ncspeed-logo" /><br>Data processing and optimization tools</div>
            </div>
        </div>
    </div>

    <div class="background-menu">
        <div id="cssmenu">
            <ul>
                <li class="button-home"><a href="/ncspeed">NCSPEED</a></li>
                <li><a href="/ncspeed/function">@Resources._Function</a></li>
                <li><a href="https://www.datadesign.co.jp/dev_ncspeed/example.html" target="_blank">Case study</a></li>
                @if (@Resources.Home == "Home")
                {
                    <li class="nav-item-1">
                        <a href="/set-culture?slug=vi&returnUrl=@returnUrl"><img src="~/Images/uploaded/flag/vietnam.png" title="Data Design Viet Nam" alt="Data Design Viet Nam"></a>

                    </li>
                }
                else
                {
                    <li class="nav-item-1">
                        <a href="/set-culture?slug=en&returnUrl=@returnUrl"><img src="~/Images/uploaded/flag/united-states.png" title="Data Design Viet Nam" alt="Data Design Viet Nam"></a>
                    </li>
                }

            </ul>
        </div>
    </div>

    @RenderBody()

    <!--start footer-->
    <div class="footer">
        Data Design VietNam Co., Ltd.<span style="margin-left: 15px; font-family: CordiaNew;">Copyright &#169; 2019 All Rights Reserved.</span>
    </div><!--end footer-->

