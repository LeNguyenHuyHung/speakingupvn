    <!--<link rel="stylesheet" href="fontello-172baab5/css/fontello.css">
    <link rel="stylesheet" href="css/styles-menu.css">-->
    @RenderSection("scripts", required: false)
    @{
        var returnUrl = ViewBag.ReturnUrl;
    }
    @if (ViewBag.Title != null)
    {
        <meta property="og:title" content="@ViewBag.Title"><!-- Main Page Title -->
    }
    else
    {
        <meta property="og:title" content="Công cụ xử lý dữ liệu và tối ưu hóa  "><!-- Main Page Title -->
    }

    @if (ViewBag.Description != null)
    {
        <meta name="description" content="@ViewBag.Description">
        <meta property="og:description" content="@ViewBag.Description">

    }
    else
    {
        <meta property="og:description" content="Ncspeed phù hợp với kiểm tra chất lượng, tối ưu hóa và các điều kiện cắt hiện có bằng cách gõ, thêm / nhận cập nhật tự động thu thập dữ liệu cho mỗi điều kiện được biên dịch, điều kiện cắt và cắt để hiển thị giải pháp sử dụng một giải pháp để sử dụng."><!-- Main Page Title -->
        <meta name="description" content="Ncspeed phù hợp với kiểm tra chất lượng, tối ưu hóa và các điều kiện cắt hiện có bằng cách gõ, thêm / nhận cập nhật tự động thu thập dữ liệu cho mỗi điều kiện được biên dịch, điều kiện cắt và cắt để hiển thị giải pháp sử dụng một giải pháp để sử dụng. ">

    }
    @if (ViewBag.Image != null)
    {
        <meta property="og:image" content="@ViewBag.Image">
    }
    else
    {
        <meta property="og:image" content="https://datadesign.vn/Content/ncspeed/images/logo/NCspeed_logo.png"><!-- Main Page Title -->

    }
    @if (ViewBag.Url != null)
    {
        <meta property="og:url" content="@ViewBag.Url">
        <link rel="canonical" href="@ViewBag.Url">
    }
    else
    {
        <meta property="og:url" content="http://datadesign.vn/ncspeed"><!-- Main Page Title -->

        <link rel="canonical" href="http://datadesign.vn/ncspeed">

    }


    <style type="text/css">
        body, td, th {
            font-family: CSChatThai;
        }

        a:link {
            text-decoration: none;
        }

        a:visited {
            text-decoration: none;
        }

        a:hover {
            text-decoration: none;
        }

        a:active {
            text-decoration: none;
        }
    </style>

