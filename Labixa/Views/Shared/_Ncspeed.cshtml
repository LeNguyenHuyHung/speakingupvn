    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="keywords" content="Data Design Việt Nam, data design, data design vietnam, Scan3D,Scan 3D,3D scanning, 3D printting, Artec 3D, markforged, Cad/CAM, datadesign">
    <meta name="author" content="Data Design Viet nam">
    <meta property="og:locale" content="vi_VN">
    <meta property="og:type" content="website"><!-- Main Page Title -->
