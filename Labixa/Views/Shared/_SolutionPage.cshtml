    @if (ViewBag.Title != null)
    {
        <meta property="og:title" content="@ViewBag.Title"><!-- Main Page Title -->
    }
    else
    {
        <meta property="og:title" content="Data Design Viet Nam"><!-- Main Page Title -->
    }

    @if (ViewBag.Description != null)
    {
        <meta name="description" content="@ViewBag.Description">
        <meta property="og:description" content="@ViewBag.Description">

    }
    else
    {
        <meta property="og:description" content="Data Design Việt Nam Công ty chuyên cung cấp giải pháp tổng thể về công nghệ 3D Scanning, Printing , cung cấp các nghiệp vụ cho dựng mô hình 3D, in 3D, thiết bị máy móc hỗ trợ về lĩnh vực 3D"><!-- Main Page Title -->
        <meta name="description" content="Data Design Việt Nam Công ty chuyên cung cấp giải pháp tổng thể về công nghệ 3D Scanning, Printing , cung cấp các nghiệp vụ cho dựng mô hình 3D, in 3D, thiết bị máy móc hỗ trợ về lĩnh vực 3D">
    }
    @if (ViewBag.Image != null)
    {
        <meta property="og:image" content="@ViewBag.Image">
    }
    else
    {
        <meta property="og:image" content="~/Content/Datadesign-home/logo1.png"><!-- Main Page Title -->
    }
    @if (ViewBag.Url != null)
    {
        <meta property="og:url" content="@ViewBag.Url">
        <link rel="canonical" href="@ViewBag.Url">
    }
    else
    {
        <meta property="og:url" content="https://datadesign.vn/solutionPage"><!-- Main Page Title -->
        <link rel="canonical" href="https://datadesign.vn/solutionPage">
    }
    <style>
        .navbar-nav .nav-item .nav-link:hover:after,
        .navbar-nav .nav-item .nav-link:focus:after,
