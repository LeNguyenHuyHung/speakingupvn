            -ms-flex-positive: 1;
            flex-grow: 1;
            -ms-flex-align: center;
            align-items: center;
        }

        .navbar-nav .nav-item-1 .nav-link-1, .navbar-nav .dropdown-menu-1 a {
            color: #fff;
            font-size: 11px;
            letter-spacing: 1px;
            text-transform: uppercase;
            font-weight: 400;
            padding: 0;
            font-family: 'poppins', sans-serif;
            margin: 12px 10px;
            position: relative;
            -webkit-transition: 0.4s ease-in-out;
