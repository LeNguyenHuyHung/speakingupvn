            -o-transition: 0.4s ease-in-out;
            transition: 0.4s ease-in-out;
            z-index: 1;
            display: block;
        }

        .dropdown-menu.show-1 {
            display: block;
        }

        .dropdown-menu-1 {
            position: absolute;
            top: 100%;
            left: 0;
            z-index: 1000;
            display: none;
            float: left;
            min-width: 10rem;
            padding: .5rem 0;
            margin: .125rem 0 0;
            font-size: 1rem;
            color: #212529;
            text-align: left;
            list-style: none;
            background-color: #fff;
            background-clip: padding-box;
            border: 1px solid rgba(0,0,0,.15);
            border-radius: .25rem;
        }
