
@{
    ViewBag.Title = "netfabbv2018";
    ViewBag.Description = "Netfabb 2018.3 đã được phát hành trong phiên bản mới. Chúng tôi sẽ giới thiệu phiên bản mới nhất của chức năng mới và nội dung cập nhật chức năng.";
    ViewBag.Image =  "https://datadesign.vn/content/autodesk/images/netfabb/netfabb-11.png"; 
    ViewBag.Url = "https://datadesign.vn/netfabbv2018";
    Layout = "~/Views/Shared/_Autodesk.cshtml";
    HttpCookie cookie = Request.Cookies["_culture"];
}
@model Outsourcing.Data.Models.Product

@section scripts{

    <link rel="stylesheet" href="/Content/autodesk/css/body_styles.css">
    <link rel="stylesheet" href="/Content/autodesk/cssmenu/styles.css">
    <link rel="stylesheet" href="/Content/Artec3D/css/reponsive_mobile.css">
    <!-- Đánh dấu JSON-LD được tạo bởi Trình trợ giúp đánh dấu dữ liệu có cấu trúc của Google. -->
    <script type="application/ld+json">
        {
        "@@context" : "http://schema.org",
        "@@type" : "Product",
        "name" : "Netfabb 2018.3",
        "description" : "Netfabb 2018.3 has been released in the new version.
        <BR />\nWe will introduce the latest version of the new function and the function update content.",
        "url" : "https://datadesign.vn/NetFabbv2018",
        "brand" : {
        "@@type" : "Brand",
        "name" : "Auto Desk",
        "logo" : "https://datadesign.vn/Content/autodesk/images/logo/netfabb-1.png"
        }
        }
    </script>
}
@if (Model != null)
{
    if (cookie.Value == "vi")
    {
        @Html.Raw(Model.Content)
    }
    else
    {
        @Html.Raw(Model.ContentEng)
    }
}
